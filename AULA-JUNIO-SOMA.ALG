algoritmo "Aula-Junio"
var
   N1, N2, S: INTEIRO
inicio
  Escreval ("Olá meu bro =)")
  Escreval ("De boa na lagoa?")
  Escreva ("Me informa um numero ai cachorro: ")
  Leia (N1)
  Escreva ("Mais outro ai dog mal: ")
  Leia (N2)
  S <- N1 + N2
  Escreva ("Boa muleque zika, a soma entre ", N1, " e ", N2, " é igual a ", S)
fimalgoritmo